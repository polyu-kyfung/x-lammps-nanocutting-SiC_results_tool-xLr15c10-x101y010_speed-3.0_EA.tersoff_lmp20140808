region trimboxfr block EDGE 63.3550224151315 EDGE ${fry} INF INF side out
region trimboxfr block EDGE 63.3550224151315 EDGE 9.07883751323322 INF INF side out
region trimboxfc block ${rx} EDGE EDGE ${fcy1} INF INF side out
region trimboxfc block 63.3550224151315 EDGE EDGE ${fcy1} INF INF side out
region trimboxfc block 63.3550224151315 EDGE EDGE 6.17488268159447 INF INF side out
region cut_toolBR union 2 cut_toolB rakeface
region cut_toolTr intersect 3 cut_toolBR trimboxfr trimboxfc
region cut_tool union 2 cut_toolTr cut_toolR

region trimfc prism ${fcx1} ${fcx2} ${fcy1} ${fcy2} INF INF ${lx} 0 0
region trimfc prism 63.8461733033795 ${fcx2} ${fcy1} ${fcy2} INF INF ${lx} 0 0
region trimfc prism 63.8461733033795 94.8461733033795 ${fcy1} ${fcy2} INF INF ${lx} 0 0
region trimfc prism 63.8461733033795 94.8461733033795 6.17488268159447 ${fcy2} INF INF ${lx} 0 0
region trimfc prism 63.8461733033795 94.8461733033795 6.17488268159447 13.9051720716069 INF INF ${lx} 0 0
region trimfc prism 63.8461733033795 94.8461733033795 6.17488268159447 13.9051720716069 INF INF 31 0 0

#define boundaries of cutting tool
region ctTop block ${dx} ${dx1} ${by1} ${dy1} INF INF
region ctTop block 60.5265971771192 ${dx1} ${by1} ${dy1} INF INF
region ctTop block 60.5265971771192 91.5265971771192 ${by1} ${dy1} INF INF
region ctTop block 60.5265971771192 91.5265971771192 36.1141136936432 ${dy1} INF INF
region ctTop block 60.5265971771192 91.5265971771192 36.1141136936432 37.1141136936432 INF INF
