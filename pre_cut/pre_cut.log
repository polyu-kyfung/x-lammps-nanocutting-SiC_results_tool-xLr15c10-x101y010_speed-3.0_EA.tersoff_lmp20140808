
###############################################################
# Input file for nanocutting of Cubic SiC with a diamond tool #
# Chris Fung, June 2013                                       #
# Input Variable:                                             #
# rA    rake angle of diamond tool (in degree)                #
###############################################################

package cuda gpu/node 1 0
package omp 2 force/neigh
using multi-threaded neighbor list subroutines
prefer double precision OpenMP force kernels

# ------------------------ INITIALIZATION ----------------------------
units 		metal
dimension	3
boundary	s	s	p
atom_style	atomic

variable a equal 4.3667  # lattice constant of 3C-SiC
variable b equal 3.571   # lattice constant of diamond
variable c equal $a/$b   # ratio of a and b
variable c equal 4.3667/$b   
variable c equal 4.3667/3.571   
variable d equal 5.05016 # lattice spacing of diamond in 110 direction
variable e equal $a/$d # lattice spacing of diamond in 110 direction
variable e equal 4.3667/$d 
variable e equal 4.3667/5.05016 
variable f equal $b/$d
variable f equal 3.571/$d
variable f equal 3.571/5.05016
variable dx equal 70*$e  # datum position x of tool region
variable dx equal 70*0.864665673958845  
variable dy equal 5*$c   # datum position y of tool region
variable dy equal 5*1.22282273872865   
variable lx equal 31     # x dimension of tool region
variable ly equal 31     # y dimension of tool region
variable dx1 equal ${dx}+${lx}   # xhi of tool region
variable dx1 equal 60.5265971771192+${lx}   
variable dx1 equal 60.5265971771192+31   
variable dy1 equal ${dy}+${ly}   # yhi of tool region
variable dy1 equal 6.11411369364325+${ly}   
variable dy1 equal 6.11411369364325+31   
variable lr equal 4      # tool edge radius
variable lrf equal 4*$f      # tool edge radius
variable lrf equal 4*0.707106309503065      
variable rx equal ${dx}+${lrf} # x-coordinate of the centre of edge radius
variable rx equal 60.5265971771192+${lrf} 
variable rx equal 60.5265971771192+2.82842523801226 
variable ry equal ${dy}+${lr}                           # y-coordinate of the centre of edge radius
variable ry equal 6.11411369364325+${lr}                           
variable ry equal 6.11411369364325+4                           
variable rakeangle equal ${rA}/180*PI   # degree of rake angle / 180 * PI
variable rakeangle equal 15/180*PI   
variable rkshiftX equal -${ly}*$f*tan(${rakeangle})   # x offset for tiling
variable rkshiftX equal -31*$f*tan(${rakeangle})   
variable rkshiftX equal -31*0.707106309503065*tan(${rakeangle})   
variable rkshiftX equal -31*0.707106309503065*tan(0.261799387799149)   
variable frx equal ${rx}-${lrf}*cos(${rakeangle})
variable frx equal 63.3550224151315-${lrf}*cos(${rakeangle})
variable frx equal 63.3550224151315-2.82842523801226*cos(${rakeangle})
variable frx equal 63.3550224151315-2.82842523801226*cos(0.261799387799149)
variable fry equal ${ry}-${lr}*sin(${rakeangle})
variable fry equal 10.1141136936433-${lr}*sin(${rakeangle})
variable fry equal 10.1141136936433-4*sin(${rakeangle})
variable fry equal 10.1141136936433-4*sin(0.261799387799149)
variable clearangle equal 10/180*PI  # degree of clearance angle / 180 * PI
variable fcx1 equal ${rx}+${lr}*$f*sin(${clearangle})  # xlo of trimfc for clearance face
variable fcx1 equal 63.3550224151315+${lr}*$f*sin(${clearangle})  
variable fcx1 equal 63.3550224151315+4*$f*sin(${clearangle})  
variable fcx1 equal 63.3550224151315+4*0.707106309503065*sin(${clearangle})  
variable fcx1 equal 63.3550224151315+4*0.707106309503065*sin(0.174532925199433)  
variable fcx2 equal ${fcx1}+${lx}                    # xhi of trimfc
variable fcx2 equal 63.8461733033795+${lx}                    
variable fcx2 equal 63.8461733033795+31                    
variable fcy1 equal ${ry}-${lr}*cos(${clearangle})  # ylo of trimfc
variable fcy1 equal 10.1141136936433-${lr}*cos(${clearangle})  
variable fcy1 equal 10.1141136936433-4*cos(${clearangle})  
variable fcy1 equal 10.1141136936433-4*cos(0.174532925199433)  
variable fcy2 equal ${fcy1}+${lx}/$f*tan(${clearangle}) # yhi of trimfc
variable fcy2 equal 6.17488268159447+${lx}/$f*tan(${clearangle}) 
variable fcy2 equal 6.17488268159447+31/$f*tan(${clearangle}) 
variable fcy2 equal 6.17488268159447+31/0.707106309503065*tan(${clearangle}) 
variable fcy2 equal 6.17488268159447+31/0.707106309503065*tan(0.174532925199433) 
variable bx1 equal ${dx1}-1                        # xlo of fixed boundary of tool region
variable bx1 equal 91.5265971771192-1                        
variable bx2 equal ${bx1}-3                        # xlo of thermostatic boundary of tool region
variable bx2 equal 90.5265971771192-3                        
variable by1 equal ${dy1}-1                        # ylo of fixed boundary of tool region
variable by1 equal 37.1141136936432-1                        
variable by2 equal ${by1}-3                        # ylo of thermostatic boundary of tool region
variable by2 equal 36.1141136936432-3                        

variable wh equal 10     # height of workpiece region
variable doc equal ${wh}-${dy}
variable doc equal 10-${dy}
variable doc equal 10-6.11411369364325

lattice fcc $a
lattice fcc 4.3667
Lattice spacing in x,y,z = 4.3667 4.3667 4.3667
region		whole block 0 99 0 47 0 7
create_box	3 whole
